{
  "name": "vue3-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "---tests---": "",
    "test:unit": "vitest --config ./unit.config.ts",
    "test:unit:coverage": "vitest --config ./unit.config.ts --coverage",
    "---liniting---": "",
    "lint": "pnpm run lint:prettier; pnpm run lint:eslint",
    "lint:prettier": "prettier --write . '!**/*-lock.yaml' --ignore-path .gitignore",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "---storybook---": "",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "immer": "^9.0.19",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-essentials": "^7.0.0-beta.46",
    "@storybook/addon-interactions": "^7.0.0-beta.46",
    "@storybook/addon-links": "^7.0.0-beta.46",
    "@storybook/blocks": "^7.0.0-beta.46",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/vue3": "^7.0.0-beta.46",
    "@storybook/vue3-vite": "^7.0.0-beta.46",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-istanbul": "^0.28.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.22.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.46",
    "tailwindcss": "^3.2.6",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  }
}
