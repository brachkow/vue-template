{
  "name": "vue-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type:check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "check:type": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "check:lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "fix:lint": "eslint --fix . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "check:format": "prettier --check .",
    "fix:format": "prettier --write .",
    "add:component": "pnpx hygen new component --folder components --name",
    "add:view": "pnpx hygen new component --folder views --name",
    "add:layout": "pnpx hygen new component --folder layouts --name",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@vueuse/core": "^10.3.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "lodash": "^4.17.21",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.2.0",
    "primevue": "^3.34.1",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "vue-i18n": "9.3.0-beta.27",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@storybook/addon-essentials": "^7.2.3",
    "@storybook/addon-interactions": "^7.2.3",
    "@storybook/addon-links": "^7.2.3",
    "@storybook/blocks": "^7.2.3",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/vue3": "^7.2.3",
    "@storybook/vue3-vite": "^7.2.3",
    "@tsconfig/node18": "^18.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.17.0",
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.2.3",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.1.6",
    "vite": "^4.4.6",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.6"
  }
}
